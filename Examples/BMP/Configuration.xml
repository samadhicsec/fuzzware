<?xml version="1.0"?>
<Configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="1.5" testConfig="true" originalFile="SmallBitMap.bmp" xmlns="urn:Fuzzware.Schemas.Configuration">
  <Input>
    <DataInputHandler>
      <XMLFileInput>
        <XMLPathAndFilename>BMP.xml</XMLPathAndFilename>
        <XSDPathAndFilename>BMP.xsd</XSDPathAndFilename>
      </XMLFileInput>
    </DataInputHandler>
  </Input>
  <XmlConfigFiles>
    <SimpleTypeFuzzerXML>SimpleTypeFuzzerConfig.xml</SimpleTypeFuzzerXML>
    <ComplexTypeFuzzerXML>ComplexTypeFuzzerConfig.xml</ComplexTypeFuzzerXML>
  </XmlConfigFiles>
  <Output>
    <ConvertFromXML>
      <OmitXMLDeclaration>true</OmitXMLDeclaration>
      <OmitComments>true</OmitComments>
      <FormatXML>false</FormatXML>
      <OutputEncoding>us-ascii</OutputEncoding>
      <VerifyOutputAgainstInputSchema>false</VerifyOutputAgainstInputSchema>
    </ConvertFromXML>
    <EvaluationMethod>
      <OutputToFile>
        <Directory>out</Directory>
        <FileExtension>bmp</FileExtension>
      </OutputToFile>
    </EvaluationMethod>
    <Monitoring />
  </Output>
</Configuration>